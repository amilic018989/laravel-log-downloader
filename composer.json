{
    "name": "shogy/laravel-log-downloader",
    "description": "Package that will easily grab all log files from your Laravel project.",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shogy\\LaravelLogDownloader\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Aleksandar Milic",
            "email": "info@aleksandarmilic.rs"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shogy\\LaravelLogDownloader\\LogDownloaderServiceProvider"
            ]
        }
    }
}
